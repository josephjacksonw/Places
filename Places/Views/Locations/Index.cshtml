<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>My To-Do List!</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
  </head>
  <body>
    @using Places.Models;

    <h1>To Do List</h1>

    @if (Model.Count == 0)
    {
      <p>There are no locations in the list.</p>
    }
    <ul>
      @foreach (Location location in Model)
      {
        <li>@location.Description</li>
        <ul>
          <li>@location.Days days stayed</li>
          <li>@location.Journal</li>
        </ul>
      }
    </ul>
    <a href="/locations/new">Add a new location.</a>
    <form action="/locations/delete" method="post">
      <button type="submit" name="button">Clear All locations</button>
    </form>
  </body>
</html>